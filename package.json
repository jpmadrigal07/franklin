{
  "name": "franklin",
  "version": "3.0.8",
  "description": "Develop by John Madrigal for Franklin's Dry and Wash Fold. To contact the developer, you can message him through Messenger.",
  "main": "server.js",
  "scripts": {
    "server": "set REACT_APP_STAGE=local && nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "prepare": "husky install",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "John Patrick M. Madrigal",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.2.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "exceljs": "^4.2.0",
    "express": "^4.17.1",
    "helpers": "0.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongoose": "^5.9.27",
    "multer": "^1.4.2",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "typescript": "^4.3.2",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "eslint": "7.11.0",
    "husky": "^7.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.3"
  }
}
